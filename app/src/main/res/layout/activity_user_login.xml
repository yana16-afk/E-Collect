<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/admin_noacctxt"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserLogin">

    <ImageView
        android:id="@+id/userlogin_bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/userlogin_bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/onboarding_bg"
        tools:ignore="ImageContrastCheck" />

    <Button
        android:id="@+id/button_sregister2"
        android:layout_width="108dp"
        android:layout_height="48dp"
        android:backgroundTint="#00903434"
        android:fontFamily="@font/poppins"
        android:text="@string/button_sregister"
        android:textAlignment="viewStart"
        android:textAllCaps="false"
        android:textColor="#1098AA"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.789"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.88"
        tools:ignore="TextContrastCheck,VisualLintOverlap" />

    <TextView
        android:id="@+id/welcomeuser_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="9dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="@string/welcomeuser_txt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.761"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.141"
        tools:ignore="VisualLintOverlap" />

    <ImageView
        android:id="@+id/ecollect_logo"
        android:layout_width="382dp"
        android:layout_height="163dp"
        android:contentDescription="@string/ecollect_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/userlogin_bg"
        app:layout_constraintVertical_bias="0.168"
        app:srcCompat="@drawable/ecollectext_logo" />

    <Button
        android:id="@+id/button_login"
        android:layout_width="243dp"
        android:layout_height="52dp"
        android:backgroundTint="#37B943"
        android:fontFamily="@font/poppins"
        android:text="@string/text_login"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/userlogin_bg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/userlogin_bg"
        app:layout_constraintTop_toBottomOf="@+id/ecollect_logo"
        app:layout_constraintVertical_bias="0.723"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/text_userregister"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="58dp"
        android:fontFamily="@font/poppins"
        android:text="@string/user_textregister"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/userlogin_bg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.101"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userlogin_img"
        app:layout_constraintVertical_bias="0.622" />

    <ImageView
        android:id="@+id/userlogin_img"
        android:layout_width="356dp"
        android:layout_height="295dp"
        android:contentDescription="@string/userlogin_img"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.421"
        app:srcCompat="@drawable/login_img" />

    <EditText
        android:id="@+id/loginuser_email"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginBottom="42dp"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:inputType="text"
        android:text="@string/userlogin_emailadd"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/button_login"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userlogin_img"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="TextFields" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="214dp"
        android:layout_height="40dp"
        android:layout_marginBottom="11dp"
        android:ems="10"
        android:inputType="textPassword"
        android:text="Password"
        app:layout_constraintBottom_toTopOf="@+id/button_login"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loginuser_email" />

</androidx.constraintlayout.widget.ConstraintLayout>